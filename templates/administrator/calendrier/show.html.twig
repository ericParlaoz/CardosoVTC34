{% extends 'base.html.twig' %}

{% block title %}Calendrier{% endblock %}

{% block body %}
    <h1>Calendrier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ calendrier.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ calendrier.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ calendrier.prenom }}</td>
            </tr>
            <tr>
                <th>Entreprise</th>
                <td>{{ calendrier.entreprise }}</td>
            </tr>
            <tr>
                <th>Rue</th>
                <td>{{ calendrier.rue }}</td>
            </tr>
            <tr>
                <th>Code_postal</th>
                <td>{{ calendrier.codePostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ calendrier.ville }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ calendrier.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ calendrier.email }}</td>
            </tr>
            <tr>
                <th>Depart</th>
                <td>{{ calendrier.depart ? calendrier.depart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ calendrier.fin ? calendrier.fin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('calendrier_index') }}">back to list</a>

    <a href="{{ path('calendrier_edit', {'id': calendrier.id}) }}">edit</a>

    {{ include('administrator/calendrier/_delete_form.html.twig') }}
{% endblock %}
